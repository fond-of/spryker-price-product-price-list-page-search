{
  "name": "fond-of-spryker/price-product-price-list-page-search",
  "description": "Price product price list page search module",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "require": {
    "php": ">=8.0",
    "fond-of-spryker/price-product-price-list": "dev-master",
    "fond-of-spryker/price-product-price-list-page-search-extension": "dev-master",
    "spryker/search": "^8.10.0",
    "spryker/synchronization-extension": "^1.0.0",
    "spryker/event": "^1.0.0 || ^2.0.0",
    "spryker/event-behavior": "^1.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^2.0",
    "php-coveralls/php-coveralls": "^2.1",
    "sebastian/phpcpd": "^6.0",
    "spryker/code-sniffer": "^0.17"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
